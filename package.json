  "license": "GPL-3.0",
  "author": "barkloaf",
  "dependencies": {
    "autosuggest-highlight": "^3.3.4",
    "bootstrap": "^4.5.0",
    "gh-pages": "^6.0.0",
    "react": "^18.2.0",
    "react-autosuggest": "^10.1.0",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
